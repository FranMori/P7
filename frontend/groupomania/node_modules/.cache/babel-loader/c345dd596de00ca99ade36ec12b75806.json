ink  to=\"/inscription\"  >Inscription</router-link>\n      <router-link  to=\"/connexion\" >Connexion</router-link>\n      <button @click=\"login()\">Connexion</button>\n      </div>\n      \n    </nav>\n    <router-view />\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"App\",\n  data: function() {\n    return {\n      userId: sessionStorage.getItem(\"userId\"),\n    };\n\n  },\n  methods: {\n    login() {\n      this.$auth.loginWithRedirect()\n    },\n    logout(){\n      this.$auth.logout({\n        returnTo: window.location.origin\n      })\n    }\n  }\n};\n</script>\n\n<style>\n#app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: white;\n  padding: 0px;\n}\n\n#header {\n  display: flex;\n  justify-content: center;\n  background-color: #2f4f4f;\n  margin-bottom: 20px;\n}\n\nhtml {\n  background-color: #708090;\n}\n\n#logo {\n  width: 300px;\n  margin-top: -100px;\n}\n\n#login {\n  display: flex;\n  flex-direction: column;\n  margin: 0% 30%;\n  border: 1px grey solid;\n  padding: 3%;\n}\n\ninput {\n  margin: 5% 0%;\n  font-size: 25px;\n}\n\nbutton {\n  margin: 5% 0%;\n  font-size: 25px;\n}\n\na {\n  text-decoration: none;\n  color: white;\n}\n\na:hover {\n  color: #2f4f4f;\n}\n\nnav {\n  display: flex;\n  justify-content: space-around;\n  margin-top: 20px;\n  margin-bottom: 50px;\n}\n\nh1 {\n  color: #2f4f4f;\n  font-size: 35px;\n  margin-bottom: 45px;\n}\n\ninput {\n  border-radius: 10px;\n  border: 0px;\n  padding: 5px;\n  background-color: #2f4f4f;\n  color: white;\n}\n\n::placeholder {\n  color: white;\n}\n\nbutton {\n  background-color: #2f4f4f;\n  border-radius: 10px;\n  color: white;\n  padding: 5px;\n  border: 0px;\n  cursor: pointer;\n}\n</style>\n"],"sourceRoot":""}]}