etUser\");\r\n  },\r\n\r\n  computed: {\r\n    ...mapState({\r\n      user: \"userInfos\",\r\n    }),\r\n  },\r\n\r\n  methods: {\r\n    switchToModifyProfile: function() {\r\n      this.mode = \"modify\";\r\n    },\r\n    modifyProfile: function() {\r\n      const self = this;\r\n      this.$store\r\n        .dispatch(\"modifyProfile\", {\r\n          prenom: this.prenom,\r\n          nom: this.nom,\r\n          email: this.email,\r\n        })\r\n        .then(function() {\r\n          self.$router.push(\"/\");\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\nh2 {\r\n  margin: 0px;\r\n}\r\n</style>\r\n"],"sourceRoot":""}]}