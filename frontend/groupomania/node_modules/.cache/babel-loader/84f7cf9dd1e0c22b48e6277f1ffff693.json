YpD,EAAZ;AACEP,MAAAA,QAAQ,CAACgD,GAAT,CAAa,sBAAsBzC,EAAnC,EACC+B,IADD,CACM,UAASC,QAAT,EAAmB;AACvBN,QAAAA,MAAM,CAAC,wBAAD,EAA2BM,QAAQ,CAACI,IAApC,CAAN;AACAe,QAAAA,OAAO,CAACC,GAAR,CAAYpB,QAAQ,CAACI,IAArB;AACD,OAJD;AAKD,KAlLQ;AAmLT2B,IAAAA,gBAAgB,EAAE,kCAAWpD,sBAAX,EAAsC;AAAA,UAApCe,MAAoC,UAApCA,MAAoC;AACtD,aAAO,IAAIC,OAAJ,CAAa,UAACC,OAAD,EAAUC,MAAV,EAAqB;AACvC,YAAMc,GAAG,GAAGC,MAAM,CAACC,QAAP,CAAgBC,IAA5B;AACA,YAAM9C,EAAE,GAAG2C,GAAG,CAACI,KAAJ,CAAU,cAAV,EAA0B,CAA1B,CAAX;AACAtD,QAAAA,QAAQ,CAACwD,GAAT,CAAa,sBAAsBjD,EAAnC,EAAuCW,sBAAvC,EACCoB,IADD,CACM,UAAUC,QAAV,EAAmB;AACvBN,UAAAA,MAAM,CAAC,WAAD,EAAc,UAAd,CAAN;AACAE,UAAAA,OAAO,CAACI,QAAD,CAAP;AACD,SAJD,EAKCC,KALD,CAKO,UAAUC,KAAV,EAAiB;AACtBR,UAAAA,MAAM,CAAC,WAAD,EAAc,cAAd,CAAN;AACAG,UAAAA,MAAM,CAACK,KAAD,CAAN;AACD,SARD;AASD,OAZM,CAAP;AAaD,KAjMQ;AAkMT8B,IAAAA,gBAAgB,EAAE,kCAAc;AAAA,UAAZtC,MAAY,UAAZA,MAAY;AAC9B,UAAMiB,GAAG,GAAGC,MAAM,CAACC,QAAP,CAAgBC,IAA5B;AACA,UAAM9C,EAAE,GAAG2C,GAAG,CAACI,KAAJ,CAAU,cAAV,EAA0B,CAA1B,CAAX;AACAtD,MAAAA,QAAQ,CAACgE,MAAT,CAAgB,sBAAsBzD,EAAtC;AACA0B,MAAAA,MAAM,CAAC,WAAD,EAAc,SAAd,CAAN;AACD;AAvMQ;AAlEe,CAAD,CAAzB;AAiRA,eAAe9B,KAAf","sourcesContent":["import {createStore} from 'vuex'\r\nconst axios = require('axios')\r\n\r\nconst instance = axios.create({\r\n  baseURL: 'http://localhost:5000/api'\r\n})\r\n\r\nconst store = createStore({\r\n  state: {\r\n    status: '',\r\n    user: {\r\n      id: -1,\r\n      token: '',\r\n    },\r\n    userInfos: {\r\n      nom: '',\r\n      prenom: '',\r\n      email: '',\r\n      password: '',\r\n      image: '',\r\n    },\r\n    textInfos: {\r\n      title: '',\r\n      text: '',\r\n\r\n    },\r\n    multimediaSubjectInfos: {\r\n      title: '',\r\n      text:'',\r\n      image:'',\r\n    },\r\n    subjects : [],\r\n    multimedias: [],\r\n  },\r\n  \r\n  mutations: {\r\n    setStatus: function (state, status) {\r\n      state.status = status\r\n\r\n    },\r\n    logUser: function (state, user) {\r\n      instance.defaults.headers.common['Authorization'] = user.token\r\n      state.user= user\r\n    },\r\n    userInfos: function (state, userInfos) {\r\n      state.userInfos = userInfos\r\n    },\r\n    subjectTextInfos: function (state, subjects) {\r\n      state.subjects = subjects\r\n    },\r\n    textInfos: function (state, textInfos) {\r\n      state.textInfos = textInfos\r\n    },\r\n    multimediaInfos: function (state, multimedias) {\r\n      state.multimedias = multimedias\r\n    },\r\n    multimediaSubjectInfos: function (state, multimediaSubjectInfos) {\r\n      state.multimediaSubjectInfos = multimediaSubjectInfos\r\n    },\r\n\r\n  },\r\n  getters: {\r\n    subjects: state => {\r\n      return state.subjects\r\n    },\r\n    multimedias: state => {\r\n      return state.multimedias\r\n    },\r\n    authenticated(state){\r\n      return !!state.user\r\n    },\r\n   \r\n  },\r\n  actions: {\r\n    // authentification\r\n    createAccount: ({commit}, userInfos) => {\r\n      return new Promise ((resolve, reject) => {\r\n        commit;\r\n        instance.post('/auth/signup', userInfos)\r\n        .then(function (response){\r\n          commit('setStatus', 'created')\r\n          resolve(response);\r\n        })\r\n        .catch(function (error) {\r\n          commit('setStatus', 'error_create')\r\n          reject(error)\r\n        })  \r\n      })\r\n    },\r\n    login: ({commit}, userInfos) => {\r\n      commit('setStatus', 'loading')\r\n      return new Promise ((resolve, reject) => {\r\n        instance.post('/auth/login', userInfos)\r\n        .then(function (response){\r\n          commit('setStatus', 'error_login')\r\n          commit('logUser', response.data)\r\n          sessionStorage.setItem('userId', response.data.userId)\r\n          resolve(response);\r\n        })\r\n        .catch(function (error) {\r\n          commit('setStatus', '')\r\n          reject(error)\r\n        });\r\n      });\r\n    },\r\n    // users\r\n    getUserInfos: ({commit}) => {\r\n      instance.get('/auth/profile')\r\n        .then(function (response){\r\n          commit('userInfos', response.data)\r\n        })\r\n        .catch(function () {\r\n        });\r\n    },\r\n    getUser : ({commit}) => {\r\n    const url = window.location.href\r\n    const id = url.split('/profile/')[1]\r\n      instance.get('/auth/profile/' + id)\r\n      .then(function(response) {\r\n        commit('userInfos', response.data)\r\n      })\r\n    },\r\n\r\n    modifyProfile: ({commit}, userInfos) => {\r\n      return new Promise ((resolve, reject) => {\r\n        const url = window.location.href\r\n        const id = url.split('/profile/')[1]\r\n        instance.put('/auth/profile/' + id, userInfos)\r\n        .then(function (response){\r\n          commit('setStatus', 'modified')\r\n          resolve(response);\r\n        })\r\n        .catch(function (error) {\r\n          commit('setStatus', 'error_modify')\r\n          reject(error)\r\n        })  \r\n      })\r\n    },\r\n    \r\n    // textSubject\r\n    createTextSubject: ({commit}, subjectTextInfos) => {\r\n      return new Promise ((resolve, reject) => {\r\n        console.log(commit)\r\n        commit;\r\n        instance.post('/auth/subject', subjectTextInfos)\r\n        .then(function (response){\r\n          \r\n          commit('setStatus', 'created')\r\n          resolve(response);\r\n        })\r\n        .catch(function (error) {\r\n          commit('setStatus', 'error_create')\r\n          reject(error)\r\n        })  \r\n      })\r\n    },\r\n    getSubjectInfos: ({commit}) => {\r\n      instance.get('/auth/subject')\r\n      .then(function (response){\r\n        commit('subjectTextInfos', response.data.subjects)\r\n        let subjects = response.data.subjects\r\n        console.log(subjects)\r\n      })\r\n      .catch(function () {\r\n      });\r\n  },\r\n    getTextSubject : ({commit}) => {\r\n    const url = window.location.href\r\n    const id = url.split('/texte/')[1]\r\n    console.log(id)\r\n      instance.get('/auth/subject/' + id)\r\n      .then(function(response) {\r\n        commit('textInfos', response.data)\r\n        console.log(response.data)\r\n      })\r\n    },\r\n    modifyTextSubject: ({commit}, textInfos) => {\r\n      return new Promise ((resolve, reject) => {\r\n        commit;\r\n        const url = window.location.href\r\n        const id = url.split('/texte/')[1]\r\n        instance.put('/auth/subject/' + id, textInfos)\r\n        .then(function (response){\r\n          commit('setStatus', 'modified')\r\n          resolve(response);\r\n        })\r\n        .catch(function (error) {\r\n          commit('setStatus', 'error_modify')\r\n          reject(error)\r\n        })  \r\n      })\r\n    },\r\n\r\n    deleteTextSubject: ({commit}) => {\r\n      const url = window.location.href\r\n      const id = url.split('/texte/')[1]\r\n      instance.delete('/auth/subject/' + id)\r\n      commit('setStatus', 'deleted')\r\n    },\r\n    // textComment\r\n    createTextComment: ({commit}, commentTextInfos) => {\r\n      return new Promise ((resolve, reject) => {\r\n        console.log(commit)\r\n        commit;\r\n        instance.post('/auth/comment', commentTextInfos)\r\n        .then(function (response){\r\n          \r\n          commit('setStatus', 'created')\r\n          resolve(response);\r\n        })\r\n        .catch(function (error) {\r\n          commit('setStatus', 'error_create')\r\n          reject(error)\r\n        })  \r\n      })\r\n    },\r\n  // Multimedia\r\ncreateMultimedia: ({commit}, multimediaInfos) => {\r\n  return new Promise ((resolve, reject) => {\r\n    console.log(commit)\r\n    instance.post('/auth/multimedia', multimediaInfos)\r\n    .then(function (response){\r\n      \r\n      commit('setStatus', 'created')\r\n      resolve(response);\r\n    })\r\n    .catch(function (error) {\r\n      commit('setStatus', 'error_create')\r\n      reject(error)\r\n    })  \r\n  })\r\n},\r\ngetMultimediaInfos: ({commit}) => {\r\n  instance.get('/auth/multimedia')\r\n  .then(function (response){\r\n    commit('multimediaInfos', response.data.multimedias)\r\n    let multimedias = response.data.multimedias\r\n    console.log(multimedias)\r\n  })\r\n  .catch(function () {\r\n  });\r\n},\r\ngetMultimedia : ({commit}) => {\r\n  const url = window.location.href\r\n  const id = url.split('/multimedia/')[1]\r\n  console.log(id)\r\n    instance.get('/auth/multimedia/' + id)\r\n    .then(function(response) {\r\n      commit('multimediaSubjectInfos', response.data)\r\n      console.log(response.data)\r\n    })\r\n  },\r\n  modifyMultimedia: ({commit}, multimediaSubjectInfos) => {\r\n    return new Promise ((resolve, reject) => {\r\n      const url = window.location.href\r\n      const id = url.split('/multimedia/')[1]\r\n      instance.put('/auth/multimedia/' + id, multimediaSubjectInfos)\r\n      .then(function (response){\r\n        commit('setStatus', 'modified')\r\n        resolve(response);\r\n      })\r\n      .catch(function (error) {\r\n        commit('setStatus', 'error_modify')\r\n        reject(error)\r\n      })  \r\n    })\r\n  },\r\n  deleteMultimedia: ({commit}) => {\r\n    const url = window.location.href\r\n    const id = url.split('/multimedia/')[1]\r\n    instance.delete('/auth/multimedia/' + id)\r\n    commit('setStatus', 'deleted')\r\n  },\r\n\r\n\r\n  }\r\n})\r\n\r\n\r\n\r\nexport default store;\r\n\r\n"]}]}