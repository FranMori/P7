.dispatch('modifyComment', FormDataCommentModify)\r\n      .then(function () {\r\n        self.$router.push('/')\r\n      })\r\n    },\r\n    cancel: function () {\r\n      const self = this\r\n      self.$router.push('/')\r\n    },\r\n    deleteComment: function() {\r\n      const self = this\r\n      this.$store.dispatch('deleteComment')\r\n      .then(function () {\r\n        self.$router.push('/')\r\n      })\r\n    }\r\n\r\n    }\r\n}\r\n</script>\r\n"],"sourceRoot":""}]}