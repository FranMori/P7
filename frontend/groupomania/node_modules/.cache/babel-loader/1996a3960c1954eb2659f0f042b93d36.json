patch('login', {\r\n        email: this.email,\r\n        password: this.password\r\n      })\r\n      .then(function () {\r\n        self.$router.push({name: 'Profile', params: {id : sessionStorage.getItem('userId')}})\r\n      }, function(error) {\r\n        console.log(error)\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n"],"sourceRoot":""}]}