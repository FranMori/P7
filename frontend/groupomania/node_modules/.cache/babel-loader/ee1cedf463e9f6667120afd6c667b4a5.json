\r\n    },\r\n    cancel: function () {\r\n      const self = this\r\n      self.$router.push('/texte')\r\n    },\r\n    deleteComment: function() {\r\n      const self = this\r\n      this.$store.dispatch('deleteTextComment')\r\n      .then(function () {\r\n        self.$router.push('/texte')\r\n      })\r\n    }\r\n\r\n    }\r\n}\r\n</script>\r\n"],"sourceRoot":""}]}