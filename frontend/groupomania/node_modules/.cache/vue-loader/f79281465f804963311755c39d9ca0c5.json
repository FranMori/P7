 </div>\n</template>\n\n<script>\nlet localStorageValue = localStorage.getItem(\"user\")\nlet localStorageValueParsed = JSON.parse(localStorageValue)\nlet userId = localStorageValueParsed.userId\n\nexport default {\n  name: \"App\",\n  data: function() {\n    return {\n      userId: userId\n    };\n\n  },\n\n  computed:{\n    currentUser() {\n      return this.$store.state.auth.user\n    }\n  },\n  methods: {\n    logout: function() {\n    this.$store.dispatch('auth/logout')\n    this.$router.push('/connexion')\n\n    },\n  },\n};\n\n</script>\n\n<style>\n#app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: white;\n  padding: 0px;\n}\n\n#header {\n  display: flex;\n  justify-content: center;\n  background-color: #2f4f4f;\n  margin-bottom: 20px;\n}\n\nhtml {\n  background-color: #708090;\n}\n\n#logo {\n  width: 300px;\n  margin-top: -100px;\n}\n\n#login {\n  display: flex;\n  flex-direction: column;\n  margin: 0% 30%;\n  border: 1px grey solid;\n  padding: 3%;\n}\n\ninput {\n  margin: 5% 0%;\n  font-size: 25px;\n}\n\nbutton {\n  margin: 5% 0%;\n  font-size: 25px;\n}\n\na {\n  text-decoration: none;\n  color: white;\n}\n\na:hover {\n  color: #2f4f4f;\n}\n\nnav {\n  display: flex;\n  justify-content: space-around;\n  margin-top: 20px;\n  margin-bottom: 50px;\n}\n\nh1 {\n  color: #2f4f4f;\n  font-size: 35px;\n  margin-bottom: 45px;\n}\n\ninput {\n  border-radius: 10px;\n  border: 0px;\n  padding: 5px;\n  background-color: #2f4f4f;\n  color: white;\n}\n\n::placeholder {\n  color: white;\n}\n\nbutton {\n  background-color: #2f4f4f;\n  border-radius: 10px;\n  color: white;\n  padding: 5px;\n  border: 0px;\n  cursor: pointer;\n}\n</style>\n"]}]}