esContent":["<template>\r\n  <div>\r\n    <h1>Votre profil</h1>\r\n    <div>\r\n      <div class=\"Profil__name\">\r\n        <h2>Prénom :</h2>\r\n        <p v-if=\"mode === 'profile'\">{{ user.prenom }}</p>\r\n        <input\r\n          v-model=\"prenom\"\r\n          name=\"prenom\"\r\n          type=\"text\"\r\n          v-else\r\n          placeholder=\"Prénom\"\r\n        />\r\n      </div>\r\n      <div>\r\n        <h2>Nom :</h2>\r\n        <p v-if=\"mode === 'profile'\">{{ user.nom }}</p>\r\n        <input v-model=\"nom\" name=\"nom\" type=\"text\" v-else placeholder=\"Nom\" />\r\n      </div>\r\n    </div>\r\n    <div>\r\n      <h2>Email :</h2>\r\n      <p v-if=\"mode === 'profile'\">{{ user.email }}</p>\r\n      <input\r\n        v-model=\"email\"\r\n        name=\"email\"\r\n        type=\"text\"\r\n        v-else\r\n        placeholder=\"Email\"\r\n      />\r\n    </div>\r\n    <div>\r\n      <h2>Photo de profil :</h2>\r\n      <img v-if=\"mode === 'profile'\" src=\"\" alt=\"Photo\" />\r\n      <form action=\"/profile\" method=\"post\" enctype=\"multipart/form-data\">\r\n      <input v-if=\"mode==='modify'\" type=\"file\" id=\"upload\"/>\r\n      <label for=\"upload\">Choisir un fichier</label>\r\n      </form>\r\n    </div>\r\n    <button v-if=\"mode === 'profile'\" @click=\"switchToModifyProfile()\">\r\n      Modifier mon profil\r\n    </button>\r\n    <button v-else @click=\"modifyProfile()\">Confirmer</button>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapState } from \"vuex\";\r\n\r\nexport default {\r\n  name: \"Profile\",\r\n  props: [\"id\"],\r\n\r\n  data() {\r\n    return {\r\n      mode: \"profile\",\r\n    };\r\n  },\r\n  mounted: function() {\r\n    this.$store.dispatch(\"getUser\");\r\n  },\r\n\r\n  computed: {\r\n    ...mapState({\r\n      user: \"userInfos\",\r\n    }),\r\n  },\r\n\r\n  methods: {\r\n    switchToModifyProfile: function() {\r\n      this.mode = \"modify\";\r\n    },\r\n    modifyProfile: function() {\r\n      c