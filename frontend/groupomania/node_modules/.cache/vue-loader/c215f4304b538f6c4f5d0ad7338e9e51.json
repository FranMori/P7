t":"","sourcesContent":["<template>\r\n<div>\r\n  <h1>Mon Profil</h1>\r\n<div>\r\n  <div class=\"Profil__name\">\r\n    <h2>Prénom :</h2>\r\n      <p v-if=\"mode === 'profile'\">{{infos.prenom}}</p>\r\n      <input type=\"text\" v-else placeholder=\"Prénom\">\r\n    </div>\r\n    <div>\r\n      <h2>Nom :</h2>\r\n      <p v-if=\"mode === 'profile'\">{{infos.nom}}</p>\r\n      <input type=\"text\" v-else placeholder=\"Nom\">\r\n  </div>\r\n    </div>\r\n    <div>\r\n      <h2>Email :</h2>\r\n      <p v-if=\"mode === 'profile'\">{{infos.email}}</p>\r\n      <input type=\"text\" v-else placeholder=\"Email\">\r\n    </div>\r\n    <div>\r\n      <h2>Photo de profil :</h2>\r\n      <img v-if=\"mode === 'profile'\" src=\"\" alt=\"Photo\">\r\n      <button v-else>Ajouter</button>\r\n    </div>\r\n    <button v-if=\"mode === 'profile'\" @click=\"switchToModifyProfile()\">Modifier mon profil</button>\r\n    <button v-else @click=\"switchToProfile()\">Confirmer</button>\r\n</div>    \r\n</template>\r\n\r\n<script>\r\n// import {mapState} from 'vuex'\r\nimport store from \"../store/index\"\r\n\r\nexport default {\r\n  name: 'Profile',\r\n  props: ['id'],\r\n  data() {\r\n    return {\r\n    profileId: this.$route.params.id \r\n    } \r\n    },\r\n    computed: {\r\n      profile() {\r\n        return store.getUserInfos.findOne(\r\n          infos => infos.id === this.profileId\r\n        )\r\n      }\r\n    },\r\n  mounted: function () {\r\n  \r\n    this.$store.dispatch('getUserInfos')\r\n  },\r\n  // data : function () {\r\n  //   return {\r\n  //     mode: 'profile',\r\n  //   }\r\n    \r\n  // },\r\n  // computed: {\r\n\r\n  //   ...mapState({\r\n  //     user: 'userInfos',\r\n  //   })\r\n\r\n  // },\r\n\r\n  methods: {\r\n    switchToModifyProfile: function () {\r\n      this.mode= 'modify'\r\n    },\r\n    switchToProfile: function () {\r\n      this.mode= 'profile'\r\n    }\r\n  }\r\n}\r\n\r\n</script>\r\n\r\n<style scoped>\r\n\r\nh2 {\r\n  margin: 0px;\r\n}\r\n\r\n</sty